{
  "expo": {
    "name": "AquaHome",
    "slug": "AquaHome_userapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "jsEngine": "hermes",
    "scheme": "aquahome",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 5,
      "googleServicesFile": "./service-account.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cherry93479.aquaHomeUi",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDBFyJk1ZsnnqxLC43WT_-OSCFZaG0OaNM"
        }
      }
    },
    "web": {
      "bundler": "metro",
    "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2927cb66-1e21-47f8-9b30-96dcf0312e2b"
      }
    },
    "owner": "cherry93479"
  }
}
